{
  "name": "nodejs-practice",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "npx nodemon",
    "dev-docker": "npm run db:create && npm run db:migrate-up && npx nodemon",
    "build": "rm -rf ./dist && tsc && tsc-alias",
    "start": "NODE_ENV=production node dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "db:create": "sequelize db:create",
    "db:migrate-up:dev": "sequelize db:migrate",
    "db:migrate-up-docker:prod": "docker exec -it $(docker ps | grep 'nodejs-code' | awk '{print $1}'  ) sh -c 'npm run db:migrate-up'",
    "db:migrate-down": "sequelize db:migrate:undo",
    "db:migrate-undo": "sequelize-cli db:migrate:undo:all"

  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.7",
    "@types/node-cron": "^3.0.11",
    "@types/nodemailer": "^6.4.15",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/web-push": "^3.6.3",
    "@typescript-eslint/eslint-plugin": "^7.6.0",
    "@typescript-eslint/parser": "^7.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.1.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "discord.js": "^14.15.3",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.9.4",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.13",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.2",
    "sequelize-cli": "^6.6.2",
    "sequelize-typescript": "^2.1.6",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uid": "^2.0.2",
    "web-push": "^3.6.7"
  }
}
